    #include <iostream>
using namespace std;

int main() {
	setlocale(LC_ALL, "Russian");
	int a;
	cin >> a;

	switch (a) {
	case 1:
	case 3:
	case 5:
	case 7:
	case 8:
	case 10:
	case 12:
		cout << "31 день";
		break;
	case 4:
	case 6:
	case 9:
	case 11:
		cout << " 30 дней";
		break;
	case 2:
		cout << "28 дней";
		break;
	}
}


7.


#include <iostream>
using namespace std;

int main() {
    setlocale(LC_ALL, "Russian");
    int n;
    cout << "Year:";
    cin >> n;

    cout << "Year ";

    switch ((n) % 10) {
    case 0:
    case 1:
        cout << "white";
        break;
    case 2:
    case 3:
        cout << "black";
        break;
    case 4:
    case 5:
        cout << "green";
        break;
    case 6:
    case 7:
        cout << "red";
        break;
    case 8:
    case 9:
        cout << "yellow";
        break;
    }


    switch ((n - 1984) % 12) {
    case 0:
        cout << "rat";
        break;
    case 1:
        cout << "cow";
        break;
    case 2:
        cout << "tiger";
        break;
    case 3:
        cout << "bunny";
        break;
    case 4:
        cout << "dragon";
        break;
    case 5:
        cout << "snake";
        break;
    case 6:
        cout << "horse";
        break;
    case 7:
        cout << "sheep";
        break;
    case 8:
        cout << "monkey";
        break;
    case 9:
        cout << "chiken";
        break;
    case 10:
        cout << "dog";
        break;
    case 11:
        cout << "pig";
        break;
    }

    return 0;
}
